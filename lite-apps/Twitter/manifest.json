{
  "manifest_version": 1,
  "lang": "en",
  "name": "Twitter",
  "start_url": "https://m.twitter.com/",
  "manifest_url": "https://hermit.chimbori.com/lite-apps/Twitter.hermit",
  "theme_color": "#4798d8",
  "secondary_color": "#378ac8",
  "hermit_bookmarks": [
    {
      "name": "__MSG_home__",
      "url": "https://mobile.twitter.com/home"
    },
    {
      "name": "__MSG_notifications__",
      "url": "https://mobile.twitter.com/notifications"
    },
    {
      "name": "__MSG_messages__",
      "url": "https://mobile.twitter.com/messages"
    },
    {
      "name": "__MSG_account__",
      "url": "https://mobile.twitter.com/account"
    },
    {
      "name": "__MSG_settings__",
      "url": "https://mobile.twitter.com/settings"
    },
    {
      "name": "__MSG_help__",
      "url": "https://support.twitter.com/"
    }
  ],
  "hermit_create": [
    {
      "name": "__MSG_tweet__",
      "url": "https://mobile.twitter.com/compose/tweet"
    }
  ],
  "hermit_search": [
    {
      "name": "__MSG_search__",
      "url": "https://twitter.com/search?q=%s"
    }
  ],
  "hermit_share": [
    {
      "name": "__MSG_tweet__",
      "url": "https://twitter.com/intent/tweet?text=%s"
    }
  ],
  "icons": [
    {
      "src": "icon.png"
    }
  ],
  "related_applications": [
    {
      "id": "com.twitter.android",
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.twitter.android"
    }
  ],
  "tags": [
    "Social"
  ]
}